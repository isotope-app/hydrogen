{
  "include": ["src", "types"],
  "compilerOptions": {
    "module": "Node16",
    "lib": ["dom", "esnext"],
    "importHelpers": true,
    "declaration": true,
    "declarationDir": "./dist",
    "sourceMap": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "emitDeclarationOnly": false,
  }
}
